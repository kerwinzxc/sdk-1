<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
<!--    <item android:state_checked="true" android:drawable="@color/white"></item>
    <item android:state_checked="false" android:drawable="@color/WanSDK_transparent"></item>
-->
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/wan669_tab_white" />
            <corners android:radius="2dip" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/wan669_tab_white_pressed" />
            <corners android:radius="2dip" />
        </shape>
    </item>

    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/wan669_tab_white_transparent" />
            <corners android:radius="2dip" />
        </shape>
    </item>


</selector>